  </jeuelement>
  <jeuelement
      xsi:type="eNIGME:Territoire"
      name="Egypte">
    <lieuDebut
        name="Enigme">
      <personnes
          name="Sphinx"
          conditionv="//@jeuelement.9">
        <texte
            qualification="Quel est la capitale du Maroc ?"/>
        <interaction
            name="Question">
          <choix
              name="debut"
              conditions="//@jeuelement.6 //@jeuelement.5">
            <reponses
                name="r1"
                etat="mauvaise"
                action="//@jeuelement.0">
              <texte
                  qualification="Ouarzazate"/>
            </reponses>
            <reponses
                name="r2"
                etat="mauvaise"
                action="//@jeuelement.0">
              <texte
                  qualification="Ifrane"/>
            </reponses>
            <reponses
                name="r3"
                etat="mauvaise"
                action="//@jeuelement.0">
              <texte
                  qualification="Casablanca"/>
            </reponses>
            <reponses
                name="r4"
                action="//@jeuelement.2">
              <texte
                  qualification="Rabat"/>
            </reponses>
          </choix>
          <choixFin
              name="choixFin">
            <reponses
                name="r1"
                action="//@jeuelement.1"/>
          </choixFin>
        </interaction>
      </personnes>
    </lieuDebut>
    <lieuxFin
        name="Succes"/>
    <lieuxFin
        name="Echec"/>
    <chemins
        conditiono="//@jeuelement.10"
        precedent="//@jeuelement.7/@lieuDebut"
        suivant="//@jeuelement.7/@lieuxFin.1"/>
    <chemins
        etatc="ferme"
        conditionc="//@jeuelement.6"
        precedent="//@jeuelement.7/@lieuDebut"
        suivant="//@jeuelement.7/@lieuxFin.0"/>
  </jeuelement>
  <jeuelement
      xsi:type="eNIGME:Connaissance"
      name="Reussite"/>
  <jeuelement
      xsi:type="eNIGME:Condition"
      name="CondVisibleSphynx">
    <objetsRequis
        name="Tentative"
        occurrence="1"/>
    <connaissancesRequises
        name="Reussite"
        etatp="npossede"/>
  </jeuelement>
  <jeuelement
      xsi:type="eNIGME:Condition"
      name="ConditionObligationCheminEchec">
