    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:eNIGME="http://www.n7.fr/ENIGME"
    name="Enigme">
  <jeuelement
